<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>

<?import javafx.scene.layout.Pane?>
<?import javafx.scene.control.Label?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ajsworton.controllers.Main">
    <VBox GridPane.columnIndex="0" GridPane.vgrow="ALWAYS" styleClass="navigationPane">
        <Button fx:id="settingsBtn" onMouseClicked="#handleClickEvent" >Settings</Button>
        <Button fx:id="screensBtn"  onMouseClicked="#handleClickEvent">Screens</Button>
    </VBox>
    <VBox GridPane.columnIndex="1" GridPane.vgrow="ALWAYS" GridPane.hgrow="ALWAYS" GridPane.valignment="CENTER" maxWidth="Infinity" styleClass="base" fx:id="contentRoot" />
</GridPane>